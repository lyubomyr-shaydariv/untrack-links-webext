{
	"name": "Just Let Me Go",
	"version": "0",
	"manifest_version": 2,
	"author": "Lyubomyr Shaydariv",
	"description": "Removes tracking parameters and bypasses tracking redirections at some sites",
	"icons" :{
		"48" : "icon-48x48.png",
		"128" : "icon-128x128.png"
	},
	"content_security_policy": "object-src 'self'; script-src 'self';",
	"background": {
		"scripts": [
			"core.js",
			"modules/_.js",
			"modules/actionmap.js",
			"modules/adobe.js",
			"modules/aliexpress.js",
			"modules/amazon.js",
			"modules/at.js",
			"modules/bilibili.js",
			"modules/bing.js",
			"modules/changeorg.js",
			"modules/drip.js",
			"modules/dpgmediagroup.js",
			"modules/duckduckgo.js",
			"modules/ebay.js",
			"modules/etsy.js",
			"modules/evernote.js",
			"modules/facebook.js",
			"modules/google.js",
			"modules/hubspot.js",
			"modules/humblebundle.js",
			"modules/ibm.js",
			"modules/imdb.js",
			"modules/instagram.js",
			"modules/itm.js",
			"modules/linkedin.js",
			"modules/mailchimp.js",
			"modules/mailerlite.js",
			"modules/mandrill.js",
			"modules/matomo.js",
			"modules/medium.js",
			"modules/microsoft.js",
			"modules/ns.js",
			"modules/omeda.js",
			"modules/oracle.js",
			"modules/reddit.js",
			"modules/sc.js",
			"modules/sourceforge.js",
			"modules/spotify.js",
			"modules/stm.js",
			"modules/tiktok.js",
			"modules/tumblr.js",
			"modules/twitch.js",
			"modules/twitter.js",
			"modules/utm.js",
			"modules/vero.js",
			"modules/vk.js",
			"modules/wickedreports.js",
			"modules/yahoo.js",
			"modules/yandex.js",
			"modules/youtube.js",
			"modules/zeit.js",
			"main.js"
		]
	},
	"permissions": [
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	]
}
